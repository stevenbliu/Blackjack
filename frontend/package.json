    "framer-motion": "^12.23.6",
    "gsap": "^3.12.7",
    "honeycomb-grid": "^4.1.5",
    "jwt-decode": "^4.0.0",
    "react": "^19.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-hexgrid": "^2.0.1",
